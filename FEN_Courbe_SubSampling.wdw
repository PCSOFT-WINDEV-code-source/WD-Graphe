        code : |1+
         
         Sablier(Vrai)
         
         // Si l'échantillonnage est actif
         si grParamètre(GRF_Graphe, grEchantillonnageActif) ALORS
         	
         	// Désactive l'échantillonnage
         	grParamètre(GRF_Graphe, grEchantillonnageActif, faux)
         	
         	// Modifie le libellé du bouton
         	moimeme.. libellé = "Activer l'échantillonnage"
         	
         SINON // Si l’échantillonnage n'est pas actif
         
         	// Active l'échantillonnage
         	grParamètre(GRF_Graphe, grEchantillonnageActif, Vrai)
         	
         	// Modifie le libellé du bouton
         	moimeme.. libellé = "Désactiver l'échantillonnage"
         
         FIn
         
         // Rafraîchit le dessin du graphe
         grDessine(GRF_Graphe)
         
         Sablier(Faux)
