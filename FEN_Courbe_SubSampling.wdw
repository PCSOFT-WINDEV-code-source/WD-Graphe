 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1-
      Procédure MaFenêtre()
      
      // Structure pour mémoriser les données
      STDonnées est une Structure
      	dDateHeure		est une DateHeure
      	nSérie1			est un entier
      Fin
      
      // Tableau contenant les données du graphe
      gtabDonnées est tableau de STDonnées
   -
     code : |1+
      
      // Masque d'affichage pour l'abscisse
      grNiveauTemporelMasque(GRF_Graphe, grntMilliseconde, "CCC")
      grNiveauTemporelMasque(GRF_Graphe, grntSeconde, "SS")
      grNiveauTemporelMasque(GRF_Graphe, grntMinute, "mm")
      grNiveauTemporelMasque(GRF_Graphe, grntHeure, "HHh")
      grNiveauTemporelMasque(GRF_Graphe, grntJour, "JJ")
      
      // Le graphe affiche les heures au max (les données s'étalent sur un unique jour)
      grNiveauTemporelMax(GRF_Graphe, 3, grntHeure)
      
      // Date de début et fin
      dhDébut est une dateheure = datedujour()+"000000"
      dhFin est une dateheure = datedujour()+"235959"
      
      // Valeur initiale
      nValeur1 est un entier = 50
      
      // Boucle pour généré des données aléatoires
      TANTQUE dhDébut <= dhFin
      	
      	// Définit la donnée
      	stUneDonnée est un stdonnées
      	stUneDonnée.dDateHeure = dhDébut
      	stUneDonnée.nSérie1 = nValeur1
      	// Mémorise la donnée
      	Ajoute(gtabDonnées, stUneDonnée)
      	
      	// Fixe la prochaine valeur
      	nValeur1 += Hasard(-2,2)
      	dhDébut..Seconde+=5
      	
      FIN
